{
  "name": "bump-please-action",
  "version": "1.1.0",
  "description": "Bump the version of a package.json files using bump-please",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "yarn package",
    "package": "yarn rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript"
  },
  "keywords": [
    "actions"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "bump-please-core": "^1.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^22.0.0",
    "rollup": "^4.46.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  }
}
